image: registry.drycc.cc/drycc/{{project}}:bookworm
{{#if build.tags}}
tags:
{{#each build.tags}}
  - {{this}}
{{/each}}
{{/if}}
manifests:
  -
    image: registry.drycc.cc/drycc/{{project}}:bookworm-amd64
    platform:
      architecture: amd64
      os: linux
  -
    image: registry.drycc.cc/drycc/{{project}}:bookworm-arm64
    platform:
      architecture: arm64
      os: linux
